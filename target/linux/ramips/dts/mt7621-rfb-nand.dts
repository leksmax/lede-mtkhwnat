/dts-v1/;

#include "mt7621.dtsi"
#include <dt-bindings/input/input.h>

/ {
	compatible = "mediatek,mt7621-rfb-nand", "mediatek,mt7621-soc";
	model = "MediaTek MT7621 RFB (NAND)";

	chosen {
		bootargs = "console=ttyS0,115200";
	};

	gpio-keys-polled {
		compatible = "gpio-keys-polled";
		#address-cells = <1>;
		#size-cells = <0>;
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio0 18 1>;
			linux,code = <KEY_RESTART>;
		};
	};
};


&raeth {
	mac-address = [00 00 00 00 00 00];
	wan_at = "p0";
	gmac1-support = "rgmii-1";
	rgmii-mode-1 = "force-1000";
	gmac2-support = "rgmii-2";
	rgmii-mode-2 = "force-1000";
	gmac2-phy-address = <0>;
	gmac1_txq_num = <1>;
	gmac1_txq_txd_num = <1024>;
	gmac2_txq_num = <1>;
	gmac2_txq_txd_num = <1024>;
	num_rx_desc = <1024>;
	status = "okay";
};

&nand {
	status = "okay";

	flash@0 {
		reg = <0>;
		nand-ecc-mode = "hw";
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "Bootloader";
				reg = <0x00000 0x0080000>;
				read-only;
			};

			partition@80000 {
				label = "Config";
				reg = <0x80000 0x0080000>;
			};

			partition@100000 {
				label = "Factory";
				reg = <0x100000 0x500000>;
			};

			partition@140000 {
				label = "firmware";
				reg = <0x600000 0x7980000>;
			};
		};
	};
};

&pcie {
	status = "okay";
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			mtk,group = "i2c", "uart3";
			mtk,function = "gpio";
		};

		uart2 {
			mtk,group = "uart2";
			mtk,function = "uart2";
		};
	};
};

&uartfull1 {
	status = "okay";
};

&sdhci {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&sdhci_pins>;
};


